                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </Pane>
      <Pane fx:id="drawingPane" onDragDetected="#setLine" onMousePressed="#mouseDown" onMouseReleased="#mouseUp" prefHeight="631.0" prefWidth="1280.0" />
