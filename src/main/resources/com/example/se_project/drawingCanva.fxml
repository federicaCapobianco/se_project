                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem fx:id="saveButton" mnemonicParsing="false" onAction="#saveFile" text="Save" />
                        <MenuItem fx:id="loadButton" mnemonicParsing="false" onAction="#loadFile" text="Load" />
